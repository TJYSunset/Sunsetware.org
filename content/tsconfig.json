{
    "compilerOptions": {
        "target": "ES2024",
        "moduleResolution": "NodeNext",
        "isolatedModules": true,
        "jsx": "react",
        "allowImportingTsExtensions": true,
        "resolveJsonModule": true,
        "moduleDetection": "force",
        "module": "Preserve",
        "allowJs": true,
        "esModuleInterop": true
    }
}
